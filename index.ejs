<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>50 Projects 50 Days</title>
  </head>

  <body>
    <div>
      <ul>
        <% names.forEach((name) => { %>
        <li>
          <a href="./<%= name %>"
            ><%= name.split("-").map(w => w[0].toUpperCase() +
            w.substring(1)).join(" ") %></a
          >
        </li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>
